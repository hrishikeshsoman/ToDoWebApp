{
  "name": "todowebapp",
  "version": "1.0.0",
  "description": "A simple todo web application.",
  "main": "index.js",
  "scripts": {
    "build:prod": "better-npm-run build:prod",
    "build:dev": "better-npm-run build:dev",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --progress --colors --inline"
  },
  "betterScripts": {
    "build:prod": {
      "command": "./node_modules/webpack/bin/webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "./node_modules/webpack/bin/webpack.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrishikeshsoman/ToDoWebApp.git"
  },
  "author": "Hrishikesh Soman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hrishikeshsoman/ToDoWebApp/issues"
  },
  "homepage": "https://github.com/hrishikeshsoman/ToDoWebApp#readme",
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "0.0.14",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^4.5.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
